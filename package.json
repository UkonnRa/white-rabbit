{
  "name": "white-rabbit",
  "version": "0.1.0",
  "license": "MIT",
  "packageManager": "yarn@3.6.3",
  "engines": {
    "yarn": ">=3",
    "node": ">=20"
  },
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "lint:base:prettier": "prettier '**/*.{vue,ts,js,json}' --ignore-path .eslintignore",
    "lint:prettier": "yarn lint:base:prettier --check",
    "lint:fix:prettier": "yarn lint:base:prettier --write",
    "lint:fix": "yarn lint:fix:prettier && yarn workspaces foreach -pitv --exclude white-rabbit run lint:fix",
    "lint": "yarn lint:prettier && yarn workspaces foreach -pitv --exclude white-rabbit run lint",
    "build": "yarn workspaces foreach -pitv --exclude white-rabbit run build",
    "upgrade": "yarn upgrade-interactive"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@tsconfig/node20": "^20.1.2",
    "@types/eslint": "^8.44.2",
    "@types/jsdom": "^21.1.2",
    "@types/node": "^20.6.0",
    "@types/prettier": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitest/coverage-c8": "^0.33.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "commitizen": "^4.3.0",
    "commitlint": "^17.7.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "stylelint": "^15.10.3",
    "stylelint-config-recommended-scss": "^13.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.4",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.8.10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
